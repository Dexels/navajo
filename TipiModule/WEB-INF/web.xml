<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>TipiModule</display-name>
  <servlet>
    <servlet-name>testservlet</servlet-name>
    <servlet-class>navajoechotipi.TestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>tipiservlet</servlet-name>
    <servlet-class>com.dexels.navajo.tipi.components.echoimpl.TipiServlet</servlet-class>
    <init-param>
      <param-name>tipidef</param-name>
      <param-value>tipi/test.xml</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>testservlet</servlet-name>
    <url-pattern>/testservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>tipiservlet</servlet-name>
    <url-pattern>/tipiservlet</url-pattern>
  </servlet-mapping>
</web-app>
