<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>
  <display-name>TipiModule</display-name>
  <servlet>
    <servlet-name>tipiservlet</servlet-name>
    <servlet-class>com.dexels.navajo.tipi.components.echoimpl.TipiServlet</servlet-class>
    <init-param>
      <param-name>tipidef</param-name>
      <param-value>tipi/start.xml</param-value>
    </init-param>
    <init-param>
      <param-name>tipidir</param-name>
      <param-value>c:/projecten/navajoechotipi/tipi</param-value>
    </init-param>
    <init-param>
      <param-name>-Dnavajo.globals.CoreSysPostmanURI=ficus:3000/sportlink/knvb/servlet/Postman</param-name>
      <param-value>-</param-value>
    </init-param>
    <init-param>
      <param-name>-Dnavajo.globals.DefaultSport=VOETBAL</param-name>
      <param-value>-</param-value>
    </init-param>
    <init-param>
      <param-name>-Dnavajo.globals.DefaultVLADatabase=vladummy/vladummy</param-name>
      <param-value>-</param-value>
    </init-param>
    <init-param>
      <param-name>-Dnavajo.globals.VLAApplicationMode=ASP</param-name>
      <param-value>-</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>tipiservlet</servlet-name>
    <url-pattern>/tipiservlet</url-pattern>
  </servlet-mapping>
</web-app>
