<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>TipiModule</display-name>
  <servlet>
    <servlet-name>testservlet</servlet-name>
    <servlet-class>navajoechotipi.TestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>tipiservlet</servlet-name>
    <servlet-class>com.dexels.navajo.tipi.components.echoimpl.TipiServlet</servlet-class>
    <init-param>
      <param-name>tipidef</param-name>
      <param-value>tipi/start.xml</param-value>
    </init-param>
    <init-param>
      <param-name>-Dnavajo.globals.CoreSysPostmanURI=fw.sportlinkservices.nl:1443/sportlink/knvb/servlet/Postman</param-name>
      <param-value>-</param-value>
    </init-param>
    <init-param>
      <param-name>-Dnavajo.globals.VLAApplicationMode=standalone -Dnavajo.globals.DefaultSport=VOETBAL</param-name>
      <param-value>-</param-value>
    </init-param>
    <init-param>
      <param-name>-Dnavajo.globals.DefaultVLADatabase=vladummy/vladummy</param-name>
      <param-value>-</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>debugjsp</servlet-name>
    <description>Added to compile JSPs with debug info</description>
    <servlet-class>org.apache.jasper.servlet.JspServlet</servlet-class>
    <init-param>
      <param-name>classdebuginfo</param-name>
      <param-value>true</param-value>
    </init-param>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>testservlet</servlet-name>
    <url-pattern>/testservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>tipiservlet</servlet-name>
    <url-pattern>/tipiservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>debugjsp</servlet-name>
    <url-pattern>*.jsp</url-pattern>
  </servlet-mapping>
</web-app>
