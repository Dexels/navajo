      </fill-image>
     </border-part>
     <border-part position="bottom">
      <fill-image>
       <image type="nextapp.echo2.app.ResourceImageReference">
        <resource-image-reference resource="/com/dexels/navajo/echoclient/resource/image/BorderBottom.png"/>
       </image>
      </fill-image>
