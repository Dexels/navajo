     <properties>
      <property name="alignment">
       <alignment horizontal="center"/>
      </property>
      <property name="background" value="#1b1b21"/>
      <property name="backgroundImage">
       <fill-image repeat="none">
        <image type="nextapp.echo2.app.ResourceImageReference">
         <resource-image-reference resource="/com/dexels/navajo/echoclient/resource/image/HeaderBackground.png"/>
        </image>
       </fill-image>
      </property>
      <property name="insets" value="8px"/>
      <property name="overflow" value="OVERFLOW_HIDDEN"/>
     </properties>
    </layout-data>
   </property>
  </properties>
 </style>
