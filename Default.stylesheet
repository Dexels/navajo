 <style name="Default" type="nextapp.echo2.app.button.AbstractButton">
  <properties>
   <property name="background" value="#4f4f5f"/>
   <property name="backgroundImage">
    <fill-image repeat="vertical">
     <image type="nextapp.echo2.app.ResourceImageReference">
