<adapterdef>
	<map>
		<tagname>binarystore</tagname>
		<object>com.dexels.navajo.adapters.binarystore.BinaryStoreAdapter</object>
		<values>
			<value name="resource" type="string" required="true" direction="in" />
			<value name="resolve" type="binary" required="false" direction="out" />
		</values>
		<methods>
			<method name="put">
				<param name="digest" field="digest" type="binary_digest" required="true" />
				<param name="value" field="value" type="binary" required="true" />
			</method>
		</methods>
	</map>
</adapterdef>
