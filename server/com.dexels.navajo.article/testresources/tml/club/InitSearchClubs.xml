   <message name="__parms__" type="simple">
      <property direction="" name="RegionCode" value="" type="string"/>
      <property direction="" name="NameSearch" value=" OR UPPER( oa.attribvalue ) LIKE ? " type="string"/>
      <property direction="" name="SearchOnOrganizationId" value="false" type="boolean"/>
      <property direction="" name="NavajoUser" value="ROOT" type="string"/>
      <property direction="" name="UpdateBy" value="ROOT/%" type="string"/>
      <property direction="" name="DatabaseProductName" value="Oracle" type="string"/>
      <property direction="" name="DatabaseVersion" value="20.1" type="string"/>
      <property direction="" name="Query" value="
            SELECT   vc.organizationid
            ,        oa.attribvalue AS searchname
            ,        vc.name
            ,        vc.shortname
            ,        vc.city
            ,        vc.communityname
            ,        vc.parentorganizationid
            ,        ( SELECT o.name
                       FROM   organization o
                       WHERE  o.organizationid = vc.parentorganizationid
                       AND    rownum           = 1
                     ) AS parentname
            ,        vc.veldregiodesc
            FROM     vw_club vc LEFT OUTER JOIN organizationattribute oa ON ( oa.organizationid = vc.organizationid AND oa.attribname = 'SEARCH_NAME' )
            WHERE    1 = 1
        AND vc.typeoforganization IN ('CLUB') AND vc.dissolveddt &gt; SYSDATE AND ( 1 = 0  OR UPPER( oa.attribvalue ) LIKE ?  )  AND ( vc.parentorganizationid LIKE UPPER( ? ) OR vc.parentorganizationid IS NULL )  ORDER BY UPPER( vc.name )" type="string"/>
   </message>
