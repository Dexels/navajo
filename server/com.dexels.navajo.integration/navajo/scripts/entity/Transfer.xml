    <property name="Country" type="string"  />
    <property name="Player" type="string" reference="navajo://Person/PersonId" key="false,required"/>
    <property name="ClubOld" type="string" reference="navajo://Club/OrganizationId" key="false,required"/>
    <property name="ClubNew" type="string" reference="navajo://Club/OrganizationId" key="false,required"/>
