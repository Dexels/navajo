<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" immediate="true" name="navajo.entity.manager">
   <implementation class="com.dexels.navajo.entity.EntityManager"/>
   <service>
      <provide interface="com.dexels.navajo.entity.EntityManager"/>
   </service>
   <reference bind="setBundleCreator" cardinality="1..1" interface="com.dexels.navajo.compiler.BundleCreator" name="BundleCreator" policy="dynamic" unbind="clearBundleCreator"/>
   <reference bind="setBundleQueue" cardinality="1..1" interface="com.dexels.navajo.compiler.tsl.BundleQueue" name="BundleQueue" policy="dynamic" unbind="clearBundleQueue"/>
   <reference bind="setDispatcher" unbind="clearDispatcher" cardinality="1..1" interface="com.dexels.navajo.server.DispatcherInterface" name="DispatcherInterface" policy="dynamic"/>
   <reference bind="setNavajoConfig" cardinality="1..1" interface="com.dexels.navajo.server.NavajoConfigInterface" name="NavajoConfigInterface" policy="dynamic" unbind="clearNavajoConfig"/>
</scr:component>
