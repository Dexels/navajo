<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" activate="activate" deactivate="deactivate" immediate="true" name="navajo.entity.listener">
   <implementation class="com.dexels.navajo.entity.listener.EntityListener"/>
   <property name="alias" type="String" value="/entity"/>
   <property name="servletName" type="String" value="entity"/>
   <service>
      <provide interface="javax.servlet.Servlet"/>
   </service>
   <reference bind="setTmlScheduler" cardinality="1..1" interface="com.dexels.navajo.script.api.TmlScheduler" name="TmlScheduler" policy="dynamic" unbind="clearTmlScheduler"/>
   <reference bind="setDispatcher" cardinality="1..1" interface="com.dexels.navajo.server.DispatcherInterface" name="DispatcherInterface" policy="dynamic" unbind="clearDispatcher"/>
   <reference bind="setEntityDispatcher" cardinality="1..1" interface="com.dexels.navajo.entity.continuations.EntityDispatcher" name="entityDispatcher" policy="dynamic" unbind="clearEntityDispatcher"/>
</scr:component>
