<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" activate="activate" immediate="true" name="navajo.entity.api.listener">
   <service>
      <provide interface="javax.servlet.Servlet"/>
   </service>
   <implementation class="com.dexels.navajo.entity.listener.EntityApiDocListener"/>
   <property name="alias" type="String" value="/entityDocumentation"/>
   <property name="servletName" type="String" value="entityDocumentation"/>
   <reference bind="setEntityManager"  unbind="clearEntityManager"  cardinality="1..1" interface="com.dexels.navajo.entity.EntityManager" name="EntityManager" policy="dynamic"/>
   <reference bind="setEntityMapper" unbind="clearEntityMapper"  cardinality="1..1" interface="com.dexels.navajo.entity.EntityMapper" name="EntityMapper" policy="dynamic"/>
   
</scr:component>
