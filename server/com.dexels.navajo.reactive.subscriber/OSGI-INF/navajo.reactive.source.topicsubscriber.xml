<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="navajo.reactive.source.subscriber">
   <implementation class="com.dexels.navajo.reactive.kafka.SubscriberReactiveSourceFactory"/>
   <service>
      <provide interface="com.dexels.navajo.reactive.api.ReactiveSourceFactory"/>
   </service>
   <property name="name" type="String" value="topicsubscriber"/>
   <reference bind="setTopicSubscriber" cardinality="1..1" interface="com.dexels.pubsub.rx2.api.TopicSubscriber" name="TopicSubscriber" policy="dynamic" unbind="clearTopicSubscriber"/>
   <reference bind="setOffsetQuery" cardinality="1..1" interface="com.dexels.kafka.api.OffsetQuery" name="OffsetQuery" policy="dynamic" unbind="clearOffsetQuery"/>
</scr:component>
