<reactive parallel="1">
	<source.sql resource="sportlinkkernel">
		<param name="query">select * from ORGANIZATION</param>
		<msg.mergeSingle.msg>
			<source.sql resource="sportlinkkernel">
				<param name="query">select * from ORGANIZATIONATTRIBUTE WHERE ORGANIZATIONID = ?</param>
				<parameval>[organizationid]</parameval>
				<message.reduce.singlemessage>
					<save to.eval="[attribname]" value.eval="[attribvalue]"/>
				</message.reduce.singlemessage>
			</source.sql>
			<store name="Attributes"/>
		</msg.mergeSingle.msg>
		<msg.take.msg count.eval="20"/>
		<msg.stream.event messageName="Club" isArray.eval="true"/>
	</source.sql>
</reactive>