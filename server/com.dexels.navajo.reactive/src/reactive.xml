<reactive>
	<source.sql resource="dummy"  array="Organization">
		<param name="query">select * from ORGANIZATION WHERE ROWNUM &lt;=20</param>
		<delete key="SHORTNAME,UPDATEBY,REMARKS"/>
		<mergeSingle>
			<source.sql resource="dummy" single="OrganizationAttribute">
				<param name="query">select * from ORGANIZATIONATTRIBUTE WHERE ORGANIZATIONID = ?</param>
				<param.eval>[ORGANIZATIONID]</param.eval>
				<reduce>
					<set key="'ATTRIBNAME'" value="ToLower([ATTRIBVALUE])" to="[ATTRIBNAME]"/>
				</reduce>
			</source.sql>
			<copyAll to="'Attributes'"/>
		</mergeSingle>
	</source.sql>
</reactive>