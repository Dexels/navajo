    <data-sets>
        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="InvoiceHeader" id="8">
            <property name="dataSource">ASP DB - DEXORA</property>
            <list-property name="parameters">
                <structure>
                    <property name="name">club1</property>
                    <property name="dataType">string</property>
                    <property name="position">1</property>
                    <expression name="defaultValue">"BBKY84H"</expression>
                    <property name="isInput">true</property>
                </structure>
                <structure>
                    <property name="name">club2</property>
                    <property name="dataType">string</property>
                    <property name="position">2</property>
                    <expression name="defaultValue">"BBKY84H"</expression>
                    <property name="isInput">true</property>
                    <property name="isOutput">false</property>
                </structure>
                <structure>
                    <property name="name">invoiceid</property>
                    <property name="dataType">integer</property>
                    <property name="position">3</property>
                    <expression name="defaultValue">1758</expression>
                    <property name="isInput">true</property>
                </structure>
            </list-property>
            <property name="queryText"> SELECT DISTINCT
                       invoiceid
                ,      invoicedate
                ,      invoicedescription
                ,      ( SELECT description
                         FROM   localcode
                         WHERE  codetable = 'instructie_type'
                         AND    code = iti1.preferredinstructiontype
                       ) AS InstructionType
                ,      ( SELECT externalid 
                         FROM   person
                         WHERE  personid = iti1.invoiceobjectid
                      ) AS personid
                ,      invoicepersonbankaccount AS accountno
                ,      invoicepersonbankaccounttype AS accounttype
                ,      invoicepersonlastname || ', ' || invoicepersoninitials || ' ' || invoicepersoninfix AS invoicepersonname
                ,      invoicepersonlastname AS lastname      
                ,      invoicepersonstreetname AS streetname
                ,      invoicepersonhousenumber AS housenumber
                ,      invoicepersonnumberappendix AS numberappendix
                ,      invoicepersoncity AS city
                ,      invoicepersonzipcode AS zipcode  
                ,      invoicedate + 14 AS followupdate                	   
                ,      invoicepaymentdate
                ,      isinvoiced
                ,      processingnumber
                ,      NVL ( ( SELECT SUM(transactionitemamount)
                               FROM   vw_invoicetransactionitems iti2
                               WHERE  iti1.invoiceid    = iti2.invoiceid
                               AND    iti2.transactionitemtype = 'D' 
                               AND    iti2.transactionitemledgerkeyid = ( SELECT ledgerkeyid 
                                                                          FROM   fin_ledgerkey 
                                                                          WHERE  ledgerkeytype = 'DEBTORS' 
                                                                        ) 
                             ), 0               
                       ) AS InvoiceAmount                    
                ,      ( SELECT attribvalue  
                         FROM   organizationattribute
                         WHERE  organizationid = ( SELECT organizationid 
                                                   FROM organization 
                                                   WHERE externalid = ? 
                                                 )
                         AND    attribid       = 'JournalCode'
                       ) AS JournalCode        
                ,      NVL ( ( SELECT description
                               FROM   fin_contribseason
                               WHERE  seasonid = ( SELECT seasonid 
                                                   FROM   fin_contribseasonperiod
                                                   WHERE  fin_contribseasonperiod.seasonid = fin_contribseason.seasonid
                                                   AND    fin_contribseasonperiod.contribseasonperiodid = iti1.contribseasonperiodid
                                                 )
                             ), '-' 
                       ) AS SeasonDescription
                ,      ( SELECT attribvalue  
                         FROM   organizationattribute
                         WHERE  organizationid = ( SELECT organizationid 
                                                   FROM organization 
                                                   WHERE externalid = ?
                                                 )
                         AND    attribid       = 'FollowUpDelay'
                       ) AS FollowUpDelay
                FROM   vw_invoicetransactionitems iti1
                WHERE  invoiceid = ?</property>
        </oda-data-set>
        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="Transaction Items" id="114">
            <property name="dataSource">ASP DB - DEXORA</property>
            <list-property name="parameters">
                <structure>
                    <property name="name">id</property>
                    <property name="dataType">integer</property>
                    <property name="position">1</property>
                    <expression name="defaultValue">1758</expression>
                    <property name="isInput">true</property>
                    <property name="isOutput">false</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT DISTINCT
                       transactionitemid
                ,      financialperiod
                ,      transactionitemdescription
                ,      transactionitemamount
                ,      ( SELECT ledgerkeynumber
                         FROM   fin_ledgerkey
                         WHERE  fin_ledgerkey.ledgerkeyid = iti1.transactionitemledgerkeyid
                       ) AS LedgerKeyNumber
                ,      transactionpersonfirstname AS firstname
                ,      transactionpersoninitials AS initials
                ,      transactionpersoninfix AS infix
                ,      transactionpersonlastname AS lastname
                FROM   vw_invoicetransactionitems iti1
                WHERE  transactionid IN ( SELECT transactionid 
                                          FROM   fin_transaction 
                                          WHERE invoiceid = ?
                                        )               
                AND    iti1.transactionitemtype = 'C'
                AND    iti1.transactionitemledgerkeyid != ( SELECT ledgerkeyid
                                                            FROM   fin_ledgerkey
                                                            WHERE  ledgerkeytype = 'DEBTORS'
                                                          )
</property>
        </oda-data-set>
