<tid>
  <tipiclass name="tipitable" class="DefaultTipiTable" package="com.dexels.navajo.tipi.impl" type="tipi">
    <events>
      <event name="onInstantiate"/>
      <event name="actionPerformed"/>
      <event name="selectionChanged"/>
    </events>
    <values>
      <value name="selectedIndex" direction="inout" type="integer"/>
      <value name="selectedMessage" direction="out" type="message"/>
    </values>
  </tipiclass>

  <tipiclass name="tipi" class="DefaultTipiPanel" package="com.dexels.navajo.tipi.impl" type="tipi">
    <events>
      <event name="onInstantiate"/>
    </events>
    <values/>
  </tipiclass>

  <tipiclass name="window" class="DefaultTipiWindow" package="com.dexels.navajo.tipi.impl" type="tipi">
    <events>
      <event name="onInstantiate"/>
      <event name="windowClose"/>
    </events>
    <values>
     <value name="title" direction="inout" type="string"/>
     <value name="x" direction="inout" type="string"/>
     <value name="y" direction="inout" type="string"/>
     <value name="h" direction="inout" type="string"/>
     <value name="w" direction="inout" type="string"/>
     <value name="closable" direction="inout" type="string"/>
     <value name="iconifiable" direction="inout" type="string"/>
     <value name="maximizable" direction="inout" type="string"/>
    </values>
  </tipiclass>

  <tipiclass name="desktop" class="DefaultTipiDesktop" package="com.dexels.navajo.tipi.impl" type="tipi">
    <events>
      <event name="onInstantiate"/>
    </events>
    <values/>
  </tipiclass>

  <tipiclass name="screen" class="DefaultTipiScreen" package="com.dexels.navajo.tipi.impl" type="tipi">
    <events>
      <event name="onInstantiate"/>
    </events>
    <values/>
  </tipiclass>

  <tipiclass name="tipitabs" class="DefaultTipiTabs" package="com.dexels.navajo.tipi.impl" type="tipi">
    <events>
      <event name="onInstantiate"/>
      <event name="selectionChanged"/>
    </events>
    <values>
      <value name="selectedIndex" direction="inout" type="integer"/>
      <value name="selectedMessage" direction="out" type="message"/>
    </values>
  </tipiclass>

  <tipiclass name="scroll" class="DefaultTipiScroller" package="com.dexels.navajo.tipi.impl" type="tipi">
    <events>
      <event name="onInstantiate"/>
    </events>
    <values/>
  </tipiclass>

  <tipiclass name="treetable" class="DefaultTipiTreeTable" package="com.dexels.navajo.tipi.impl" type="tipi">
    <events>
      <event name="onInstantiate"/>
    </events>
    <values/>
  </tipiclass>

  <tipiclass name="button" class="TipiButton" package="com.dexels.navajo.tipi.components" type="component">
    <events>
      <event name="onInstantiate"/>
      <event name="onActionPerformed"/>
      <event name="onMouseEntered"/>
      <event name="onMouseExited"/>
    </events>
    <values>
     <value name="text" direction="inout" type="string"/>
    </values>
  </tipiclass>

  <tipiclass name="label" class="TipiLabel" package="com.dexels.navajo.tipi.components" type="component">
    <events>
      <event name="onInstantiate"/>
    </events>
    <values>
     <value name="text" direction="text" type="string"/>
    </values>
  </tipiclass>

  <tipiclass name="toolbar" class="DefaultTipiToolBar" package="com.dexels.navajo.tipi.impl" type="component">
    <events>
      <event name="onInstantiate"/>
    </events>
    <values>
     <value name="orientation" direction="text" type="string"/>
    </values>
  </tipiclass>


  <tipiclass name="table" class="DefaultTipiTableLayout" package="com.dexels.navajo.tipi.impl" type="layout"/>
  <tipiclass name="default" class="DefaultColumnLayout" package="com.dexels.navajo.tipi.impl" type="layout"/>
  <tipiclass name="border" class="DefaultTipiBorderLayout" package="com.dexels.navajo.tipi.impl" type="layout"/>

  <tipiclass name="hidden" class="TipiLabel" package="com.dexels.navajo.tipi.components" type="component"/>
  <tipiclass name="property" class="BasePropertyComponent" package="com.dexels.navajo.tipi.components" type="component"/>

  <tipiclass name="error" class="DefaultTipiErrorHandler" package="com.dexels.navajo.tipi" type="tipi"/>

</tid>