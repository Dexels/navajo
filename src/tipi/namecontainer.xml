<tid startscreen="start">

  <!-- Container definitions -->
  <container name="test">
    <table>
      <tr>
        <td><property name="FirstName"/></td>
        <td><property name="LastName"/></td>
        <td><property name="Infix"/></td>
      </tr>
    </table>
  </container>

  <!-- Tipi definitions -->
  <tipi name="adres" service="ProcessQueryMember">
    <table>
      <tr>
         <td><container-instance name="test" prefix="/MemberData"/></td>
         <td><container-instance name="test"/></td>
         <td><container-instance name="test"/></td>
      </tr>
      <tr>
         <td><container-instance name="test"/></td>
         <td><tipi-instance name="fiets"/></td>
         <td><tipi-instance name="fiets"/></td>
      </tr>
    </table>
  </tipi>
  <tipi name="fiets">
    <table>
      <tr>
         <td><container-instance name="test"/></td>
      </tr>
      <tr>
         <td><container-instance name="test"/></td>
      </tr>
    </table>
  </tipi>

  <!-- Screen definitions -->
  <screen name="start">
    <table>
      <tr>
         <td><tipi-instance name="adres" inputmessage=""/></td>
      </tr>
    </table>
  </screen>
</tid>
