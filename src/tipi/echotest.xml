<tid errorhandler="error" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="tipiscript.xsd">
    <tipi-include location="com/dexels/navajo/tipi/classdef.xml"/>
    <tipi-include location="com/dexels/navajo/tipi/components/echoimpl/echoclassdef.xml"/>
    <tipi-include location="com/dexels/navajo/tipi/actions/actiondef.xml"/>
    <tipi-include location="com/dexels/navajo/tipi/actions/echoactiondef.xml"/>
    <client-config config="server.xml" impl="indirect" password=""
        server="spiritus/sportlink/knvb/servlet/Postman" username="ROOT"/>

    <tipi centered="true" class="frame" fullscreen="false" h="600" id="init" name="init" service=""
        title="&apos;Tipi Demo&apos;" w="800">
        <tipi-instance id="menu" name="mymenu"/>
        <layout type="border">
            <tipi-instance class="tabs" constraint="Center" id="tabs" service="">
                <tipi-instance bottom="5" class="tipi" id="tipi1" left="5" right="5"
                    service="ProcessQueryMember" top="5">
                    <layout type="gridbag">
                        <component-instance capitalization="off" class="property"
                            constraint="0,3,1,1,1.0,1.0,17,2,1,1,1,1,0,0" id="BirthDate"
                            label_indent="100" label_valign="top"
                            propertyname="&apos;/MemberData/BirthDate&apos;" showlabel="true"/>
                        <component-instance capitalization="off" class="property"
                            constraint="0,2,1,1,1.0,1.0,17,2,1,1,1,1,0,0" id="FirstInitials"
                            label_indent="100" label_valign="top"
                            propertyname="&apos;/MemberData/FirstInitials&apos;" showlabel="true"/>
                        <component-instance capitalization="off" class="property"
                            constraint="0,0,1,1,1.0,1.0,17,2,1,1,1,1,0,0" id="FirstName"
                            label_indent="100" label_valign="top"
                            propertyname="&apos;/MemberData/FirstName&apos;" showlabel="true"/>
                        <component-instance class="button"
                            constraint="0,4,1,1,1.0,1.0,17,2,1,1,1,1,0,0" id="button"/>
                        <component-instance capitalization="off" class="property"
                            constraint="0,1,1,1,1.0,1.0,17,2,1,1,1,1,0,0" id="LastName"
                            label_indent="100" label_valign="top"
                            propertyname="&apos;/MemberData/LastName&apos;" showlabel="true"/>
                    </layout>
                </tipi-instance>
                <tipi-instance bottom="5" class="tipi" id="tipi" left="5" right="5"
                    service="InitUpdateMember" top="5">
                    <layout type="gridbag">
                        <component-instance capitalization="off" class="property"
                            constraint="0,0,1,1,1.0,1.0,17,2,1,1,1,1,0,0" id="MemberIdentifier"
                            label_indent="100" label_valign="top"
                            propertyname="&apos;/QueryUpdateMember/MemberIdentifier&apos;" showlabel="true"/>
                    </layout>
                </tipi-instance>
            </tipi-instance>
        </layout>
    </tipi>
    <component class="menubar" name="mymenu">
        <component-instance class="menu" id="Programma" mnemonic="'r'" text="'Programma'">
            <component-instance accelerator="'alt F4'" class="menuitem" id="Afsluiten"
                mnemonic="'a'" text="'Afsluiten'">
                <event type="onActionPerformed">
                    <action type="showQuestion">
                        <param name="text" value="'Weet u het zeker?'"/>
                    </action>
<!--                    <action type="exit"/>-->
                </event>
            </component-instance>
        </component-instance>
        <component-instance class="menu" id="Systeem" mnemonic="'s'" text="'Systeem'">
            <component-instance class="menuitem" id="Initiele download" mnemonic="'e'" text="'Eerste download'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'migrateWindow'"/>
                        <param name="name" value="'migrateWindow'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="force" value="false"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/desktop/query}"/>
                        <param name="method" value="'vla/InitMigrateOrganization'"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="{tipi://init/desktop/migrateWindow}"/>
                        <param name="name" value="'show'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance class="menuitem" id="Synchronisatie" mnemonic="'g'" text="'Gegevens uitwisselen'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'synchronizationWindow'"/>
                        <param name="name" value="'synchronizationWindow'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="force" value="true"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="{tipi://init/desktop/synchronizationWindow}"/>
                        <param name="name" value="'show'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance class="menuitem" id="Codetabellen Downloaden" mnemonic="'c'" text="'Codetabellen downloaden'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'codeTableMigrateWindow'"/>
                        <param name="name" value="'codeTableMigrateWindow'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="force" value="true"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/desktop/query}"/>
                        <param name="method" value="'vla/InitMigrateOrganization'"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="{tipi://init/desktop/codeTableMigrateWindow}"/>
                        <param name="name" value="'show'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance class="menuitem" id="Wachtwoord wijzigen" mnemonic="'w'" text="'Wachtwoord wijzigen'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'userAdmin'"/>
                        <param name="name" value="'userAdmin'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="force" value="false"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/desktop/query}"/>
                        <param name="method" value="'vla/InitVLAInsertUser'"/>
                    </action>
                </event>
            </component-instance>
        </component-instance>
        <component-instance class="menu" id="Lid" mnemonic="'p'" text="'Persoon'">
            <component-instance accelerator="'control Z'" class="menuitem" id="Search"
                mnemonic="'z'" text="'Zoekscherm leden'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'mainFrameSearch'"/>
                        <param name="name" value="'mainFrameSearch'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="visible" value="true"/>
                    </action>
                </event>
            </component-instance>
            <component-instance accelerator="'control L'" class="menuitem" id="Aanmelden"
                mnemonic="'l'" text="'Aanmelden Lid'">
                <event type="onActionPerformed">
                    <action type="debug">
                        <param name="value" value="'---------------------------------&gt;  Vuur!'"/>
                    </action>
                    <action type="instantiate">
                        <param name="id" value="'insertMemberFrame'"/>
                        <param name="name" value="'insertMemberFrame'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="{tipi://init/desktop/insertMemberFrame}"/>
                        <param name="name" value="'show'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance accelerator="'control R'" class="menuitem" id="AanmeldenRel"
                mnemonic="'r'" text="'Aanmelden Relatie'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'insertRelationFrame'"/>
                        <param name="name" value="'insertRelationFrame'"/>
                        <param name="location" value="'tipi://init/desktop'"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="{tipi://init/desktop/insertRelationFrame}"/>
                        <param name="name" value="'show'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance accelerator="'control ENTER'" class="menuitem"
                id="Bekijken/Wijzigen" mnemonic="'b'" text="'Bekijken/Wijzigen'">
                <event type="onActionPerformed">
                    <condition>
                        <param name="tipipath" value="message://init/desktop/memberFrameSearch/overview:selectedMessage"/>
                        <param name="expression" value="!?[/Members/Status]"/>
                        <action type="showInfo">
                            <param name="text" value="'U dient eerst een lid te selecteren door eenmaal op de betreffende rij te klikken'"/>
                        </action>
                        <action type="break"/>
                    </condition>
                    <condition>
                        <param name="tipipath" value="message:/init/desktop/memberFrameSearch/overview:selectedMessage"/>
                        <param name="expression" value="[/Members/Status] == 'parked' OR [/Members/Status] == 'insync_removed'"/>
                        <action type="showInfo">
                            <param name="text" value="'De gegevens van dit lid [' + {property:/init/desktop/memberFrameSearch/overview:selectedMessage:/Name} + '] kunnen niet worden gewijzigd'"/>
                        </action>
                    </condition>
                    <action type="setValue">
                        <param name="from" value="{property://init/desktop/memberFrameSearch/overview:selectedMessage:/PersonId}"/>
                        <param name="to" value="{propertyref://init/messagestore:.:/QueryUpdateMember/PersonId}"/>
                    </action>
                    <action type="instantiate">
                        <param name="id" value="'memberFrame'"/>
                        <param name="name" value="'memberFrame'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <!--<param name="force" value="true"/>-->
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/messagestore}"/>
                        <param name="method" value="'vla/ProcessVLAQueryMember'"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/messagestore}"/>
                        <param name="method" value="'vla/ProcessVLAGetClubMemberships'"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/messagestore}"/>
                        <param name="method" value="'vla/ProcessVLAQueryMemberFunctions'"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/messagestore}"/>
                        <param name="method" value="'vla/ProcessVLAQueryClubTrainers'"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/messagestore}"/>
                        <param name="method" value="'vla/ProcessVLAQueryClubMembershipActivities'"/>
                    </action>
                    <!--<action type="performTipiMethod">
                      <param name="path" value="tipi://init/desktop/memberFrame/tabs/contributionData/memberContributionTable"/>
                      <param name="name" value="update"/>
                    </action>-->
                    <condition>
                        <param name="tipipath" value="message://init/desktop/memberFrameSearch/overview:selectedMessage"/>
                        <param name="expression" value="[RoleId] == 'RELATION'"/>
                        <action type="debug">
                            <param name="value" value="'-&gt; It is a RELATION'"/>
                        </action>
                        <action type="setValue">
                            <param name="to" value="{attributeref://init/desktop/relationFrame:visible}"/>
                            <param name="from" value="true"/>
                        </action>
                        <action type="setValue">
                            <param name="to" value="{attributeref://init/desktop/relationFrame:selected}"/>
                            <param name="from" value="true"/>
                        </action>
                        <action type="setValue">
                            <param name="to" value="{attributeref://init/desktop/memberFrame:visible}"/>
                            <param name="from" value="false"/>
                        </action>
                    </condition>
                    <condition>
                        <param name="tipipath" value="message://init/desktop/memberFrameSearch/overview:selectedMessage"/>
                        <param name="expression" value="[RoleId] == 'MEMBER'"/>
                        <action type="debug">
                            <param name="value" value="'-&gt; It is a MEMBER'"/>
                        </action>
                        <action type="setValue">
                            <param name="to" value="{attributeref://init/desktop/memberFrame:visible}"/>
                            <param name="from" value="true"/>
                        </action>
                        <action type="setValue">
                            <param name="to" value="{attributeref://init/desktop/memberFrame:selected}"/>
                            <param name="from" value="true"/>
                        </action>
                        <action type="setValue">
                            <param name="to" value="{attributeref://init/desktop/relationFrame:visible}"/>
                            <param name="from" value="false"/>
                        </action>
                    </condition>
                </event>
            </component-instance>
            <!--<component-instance class="menuitem" name="Afmelden" text="'Afmelden'"/>-->
        </component-instance>
        <component-instance class="menu" enabled="false" id="Contribution" text="'Contributie'">
            <component-instance class="menuitem" name="Profielen" text="'Profielen'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'contributieProfielenFrame'"/>
                        <param name="name" value="'contributieProfielenFrame'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="visible" value="true"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/desktop/query}"/>
                        <param name="method" value="'vla/ProcessVLAQueryContributionProfiles'"/>
                    </action>
                </event>
            </component-instance>
        </component-instance>
        <!--<component-instance class="menu" name="Lid" text="'Team'"/>-->
        <component-instance class="menu" id="Afdrukken" mnemonic="'a'" text="'Afdrukken'">
            <component-instance class="menuitem" enabled="false" id="LMK" mnemonic="'m'" text="'LMK'">
                <event type="onActionPerformed">
                    <condition>
                        <param name="tipipath" value="message://init/messagestore"/>
                        <param name="expression" value="[/__globals__/VLAApplicationMode] != 'ASP'"/>
                        <action type="setValue">
                            <param name="to" value="{attributeref://mymenu/Afdrukken/LMK:enabled}"/>
                            <param name="from" value="true"/>
                        </action>
                    </condition>
                </event>
                <event type="onActionPerformed">
                    <!--<condition>
                      <param name="tipipath" value="message://init/desktop/memberFrameSearch/overview:selectedMessage"/>
                      <param name="expression" value="[/Members/Status] != 'gewijzigd'"/>
                      <action type="showInfo">
                          <param name="text" value="'LMK kan alleen worden uitgeprint voor gewijzigde leden, vanuit het zoekresultaten scherm'"/>
                      </action>
                      <action type="break"/>
                  </condition>-->
                    <action type="setValue">
                        <param name="from" value="{property://init/desktop/memberFrameSearch/overview:selectedMessage:/PersonId}"/>
                        <param name="to" value="{propertyref://init/messagestore:.:/QueryUpdateMember/PersonId}"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/messagestore}"/>
                        <param name="method" value="'vla/ProcessVLAGetUpdateRequest'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance class="menuitem" id="Ledengegevens" mnemonic="'l'" text="'Ledengegevens'">
                <event type="onActionPerformed">
                   <action type="showInfo">
                        <param name="text" value="'Ja hoor, dat gaat echt lukken'"/>
                    </action>
                    <!--<action type="copyValue">
                        <param name="from_path" value="{property://init/desktop/mainFrameSearch/SearchCriteria:.:/Club/ClubDescription}"/>
                        <param name="to_path" value="attribute://init/desktop/tableprinter:subtitle"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="tipi://init/desktop/tableprinter"/>
                        <param name="name" value="print"/>
                        <param name="tablepath" value="tipi://init/desktop/memberFrameSearch/overview"/>
                        <param name="title" value="Ledenlijst"/>
                    </action>-->
                </event>
            </component-instance>
            <component-instance class="menuitem" id="Functionarissen" mnemonic="'f'" text="'Functionarissen'">
                <event type="onActionPerformed">
                    <!--                    <action type="instantiate">
                        <param name="id" value="'functionarissen'"/>
                        <param name="name" value="'functionarissen'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                    </action>-->
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/desktop/query}"/>
                        <param name="method" value="'vla/ProcessVLAGetFunctionarissen'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance class="menuitem" id="Adreslabels" mnemonic="'a'" text="'Adreslabels'">
                <action type="instantiate">
                    <param name="id" value="'etiketten'"/>
                    <param name="name" value="'etiketten'"/>
                    <param name="location" value="{tipi://init/desktop}"/>
                </action>
                <event type="onActionPerformed">
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/desktop/mainFrameSearch/SearchCriteria}"/>
                        <param name="method" value="'vla/ProcessVLAGetAddressLabels'"/>
                    </action>
                </event>
            </component-instance>
        </component-instance>
        <component-instance class="menu" id="Exporteren" mnemonic="'e'" text="'Exporteren'">
            <component-instance accelerator="'control E'" class="menuitem" id="Exporteren.."
                mnemonic="'e'" text="'Exporteren..'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'exportdialog'"/>
                        <param name="name" value="'exportdialog'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="visible" value="true"/>
                        <param name="force" value="true"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/desktop/mainFrameSearch/SearchCriteria}"/>
                        <param name="method" value="'vla/ProcessVLASearchMembersExport'"/>
                    </action>
                </event>
            </component-instance>
        </component-instance>
        <component-instance class="menu" id="Help" mnemonic="'h'" text="'Help'">
            <component-instance accelerator="'F1'" class="menuitem" id="Help" mnemonic="'h'" text="'Help'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'helpWindow'"/>
                        <param name="name" value="'helpWindow'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="force" value="true"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="{tipi://init/desktop/helpWindow}"/>
                        <param name="name" value="'show'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance accelerator="'F2'" class="menuitem" id="Certificaat" mnemonic="'c'" text="'Certificaat'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'certificateWindow'"/>
                        <param name="name" value="'certificateWindow'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="force" value="true"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="{tipi://init/desktop/certificateWindow}"/>
                        <param name="name" value="'show'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance accelerator="'control H'" class="menuitem" id="Info" mnemonic="'i'" text="'Info'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'infoWindow'"/>
                        <param name="name" value="'infoWindow'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="visible" value="true"/>
                        <param name="force" value="false"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="{tipi://init/desktop/infoWindow}"/>
                        <param name="name" value="'show'"/>
                    </action>
                </event>
            </component-instance>
            <component-instance class="menuitem" id="Info" text="'Systeem'">
                <event type="onActionPerformed">
                    <action type="instantiate">
                        <param name="id" value="'systemWindow'"/>
                        <param name="name" value="'systemWindow'"/>
                        <param name="location" value="{tipi://init/desktop}"/>
                        <param name="visible" value="true"/>
                        <param name="force" value="false"/>
                    </action>
                    <action type="performTipiMethod">
                        <param name="path" value="{tipi://init/desktop/systemWindow}"/>
                        <param name="name" value="'show'"/>
                    </action>
                    <action type="performMethod">
                        <param name="tipipath" value="{tipi://init/desktop/query}"/>
                        <param name="method" value="'vla/ProcessGetNetworkInformation'"/>
                    </action>
                </event>
            </component-instance>
        </component-instance>
    </component>
</tid>
