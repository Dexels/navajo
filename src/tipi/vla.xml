<!-- POPUP Definitions -->
<popup name="deskpopup">
    <menuitem name="Init">
    <event type="onActionPerformed">
      <action type="reset" >
        <param name="tipipath" value="/InitSearchMembersWindow/InitSearchMembersTipi"/>
      </action>
    </event>
    </menuitem>
    <menuitem name="Open">
    <event type="onActionPerformed">
      <action type="performmethod">
        <param name="tipipath" value="/InitSearchMembersWindow/InitSearchMembersTipi"/>
        <param name="method" value="ProcessSearchMembers"/>
      </action>
      <action type="setvisible">
        <param name="tipipath" value="/ProcessSearchMembersWindow"/>
        <param name="value" value="true"/>
      </action>
    </event>
    </menuitem>
    <menuitem name="Exit">
    <event type="onActionPerformed">
      <action type="showquestion" >
        <param name="value" value="Weet u het zeker?"/>
      </action>
      <action type="exit"/>
    </event>
    </menuitem>
</popup>

<!-- MENUBAR Definitions -->
<menubar name="mymenu">
<menu name="file">
    <menuitem name="Init">
    <event type="onActionPerformed">
      <action type="callService" >
        <param name="service" value="InitSearchMembers"/>
      </action>
    </event>
    </menuitem>
    <menuitem name="Open">
    <event type="onActionPerformed">
      <action type="performmethod" >
        <param name="tipipath" value="/InitSearchMembersWindow/InitSearchMembersTipi"/>
        <param name="method" value="ProcessSearchMembers"/>
      </action>
      <action type="setvisible">
        <param name="tipipath" value="/ProcessSearchMembersWindow"/>
        <param name="value" value="true"/>
      </action>
    </event>
    </menuitem>
    <menuitem name="Exit">
    <event type="onActionPerformed">
      <action type="showquestion" >
        <param name="value" value="Weet u het zeker?"/>
      </action>
      <action type="exit"/>
    </event>
    </menuitem>
</menu>
</menubar>

<!-- TIPICLASS Definitions -->
