<tid startscreen="init" title="TipiVLA, extra creamy!">
<screen-instance name="init" fullscreen="true"/>

<!-- TIPI Definitions -->
<tipi name="init" class="screen" menubar="mymenu">
  <layout type="table">
    <tr>
      <td weighty="100"><tipi-instance name="tabs"/></td>
    </tr>
    <tr>
      <td weighty="1"><tipi-instance name="okcancel"/></td>
    </tr>
  </layout>
</tipi>

<tipi name="tabs" type="tabbed" class="tipitabs" popup="deskpopup" service="InitVLA" autoload="true">
  <tipi-instance name="membertab" title="Status" scrollable="true"/>
  <tipi-instance name="memberpersonaldata" title="Personal" prefix="/MemberData"/>
  <tipi-instance name="memberfunctions" title="Functions" prefix="/MemberData"/>
  <tipi-instance name="membersportdetails" title="Sportdetails" prefix="/MemberData"/>
  <tipi-instance name="memberaddressdata" title="Address" prefix="/MemberData" border="true"/>
  <tipi-instance name="membershipfinan" title="Financial+Memberships" prefix="/MemberData"/>
</tipi>

<tipi name="okcancel" class="tipi">
  <layout type="table">
    <tr>
      <td weighty="0"><component type="hidden"/></td>
      <td height="5" weightx="0" weighty="0"><button-instance name="okButton"/></td>
      <td height="5" weightx="0" weighty="0"><button-instance name="cancelButton"/></td>
    </tr>
  </layout>
</tipi>

<button name="okButton" value="Ok">
  <event type="onActionPerformed">
    <action type="showquestion" >
      <param name="value" value="Weet u het zeker?"/>
    </action>
    <action type="exit"/>
  </event>
</button>

<button name="cancelButton" value="Annuleren">
  <event type="onActionPerformed">
    <action type="showquestion" >
      <param name="value" value="Weet u het zeker?"/>
    </action>
    <action type="exit"/>
  </event>
</button>

<tipi name="membertab" class="desktop">
  <tipi-instance name="memberwindow" title="Status" prefix="/MemberData"/>
</tipi>
<tipi name="memberwindow" x="100" y="100" w="500" h="400" class="window" title="Flappie">
  <tipi-instance name="memberstatus" title="Status" prefix="/MemberData"/>
</tipi>

