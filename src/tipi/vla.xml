<tid startscreen="init">

  <!-- Screen definition -->
  <screen name="init">
    <table>
      <tr>
        <td cellpadding="2"><tipi-instance name="updatemember"/></td>
      </tr>
    </table>
  </screen>

  <!-- Tipi definitions -->
  <tipi name="updatemember" service="InitUpdateMember">
    <table>
      <tr>
        <td>
          <container-instance name="member_relation" prefix="/QueryUpdateMember"/>
        </td>
        <td>
          <container-instance name="member_name"/>
        </td>
        <td>
          <container-instance name="member_functions"/>
        </td>
      </tr>
      <tr>
        <td>
          <container-instance name="member_game"/>
        </td>
        <td>
          <container-instance name="member_address"/>
        </td>
        <td>
          <container-instance name="member_clubfinan"/>
        </td>
      </tr>
    </table>
  </tipi>


  <!-- Container definitions -->
  <container name="member_relation">
    <table>
      <tr>
        <td align="left"><property name="MemberIdentifier"/></td>
      </tr>
      <tr>
        <td align="left"><property name="RegistrationDate"/></td>
      </tr>
      <tr>
        <td align="left"><property name="DeregistrationDate"/></td>
      </tr>
      <tr>
        <td align="left"><property name="MemberStatus"/></td>
      </tr>
      <tr>
        <td align="left"><property name="ProcessingDate"/></td>
      </tr>
      <tr>
        <td align="left"><property name="ProcessingStatus"/></td>
      </tr>
    </table>
  </container>

  <container name="member_name">
    <table>
      <tr>
        <td colspan="2" align="left"><property name="LastName"/></td>
      </tr>
      <tr>
        <td align="left"><property name="FirstName"/></td>
        <td align="left"><property name="FirstInitials"/></td>
      </tr>
      <tr>
        <td align="left"><property name="Infix"/></td>
      </tr>
      <tr>
        <td align="left"><property name="Gender"/></td>
      </tr>
      <tr>
        <td align="left"><property name="BirtDate"/></td>
      </tr>
      <tr>
        <td colspan="2" align="left"><property name="BirthPlace"/></td>
      </tr>
      <tr>
        <td colspan="2" align="left"><property name="Nationality"/></td>
      </tr>
      <tr>
        <td colspan="2" align="left"><property name="IdentificationType"/></td>
      </tr>
      <tr>
        <td colspan="2" align="left"><property name="IdentificationNumber"/></td>
      </tr>
    </table>
  </container>

  <container name="member_functions"/>

  <container name="member_game">
    <table>
      <tr>
        <td align="left"><property name="isSaturdayMember"/></td>
      </tr>
      <tr>
        <td align="left"><property name="isSundayMember"/></td>
      </tr>
      <tr>
        <td align="left"><property name="isNonPlayingMember"/></td>
      </tr>
      <tr>
        <td align="left"><property name="isFutsalMember"/></td>
      </tr>
      <tr>
        <td align="left"><property name="isRecreationalMember"/></td>
      </tr>
      <tr>
        <td align="left"><property name="isBZAMember"/></td>
      </tr>
      <tr>
        <td align="left"><property name="isHonoraryMember"/></td>
      </tr>
      <tr>
        <td align="left"><property name="OtherSports"/></td>
      </tr>
      <tr>
        <td align="left"><property name="Trainer"/></td>
      </tr>
      <tr>
        <td align="left"><property name="AgeGroup"/></td>
      </tr>
    </table>
  </container>

  <container name="member_address">
    <table>
      <tr>
        <td colspan="2" align="left"><property name="StreetName"/></td>
      </tr>
      <tr>
        <td align="left"><property name="AddressNumber"/></td>
        <td align="left"><property name="AddressNumberAppendix"/></td>
      </tr>
      <tr>
        <td align="left"><property name="ZipCode"/></td>
      </tr>
      <tr>
        <td colspan="2" align="left"><property name="City"/></td>
      </tr>
      <tr>
        <td colspan="2" align="left"><property name="AddressCountry"/></td>
      </tr>
      <tr>
        <td align="left"><property name="HomeTelephone"/></td>
      </tr>
      <tr>
        <td align="left"><property name="AlternateHomeTelephone"/></td>
      </tr>
      <tr>
        <td align="left"><property name="HomeTelephoneType"/></td>
      </tr>
      <tr>
        <td align="left"><property name="WorkTelephone"/></td>
      </tr>
      <tr>
        <td align="left"><property name="AlternateWorkTelephone"/></td>
      </tr>
      <tr>
        <td align="left"><property name="WorkTelephoneType"/></td>
      </tr>
    </table>
  </container>

  <container name="member_clubfinan"/>

</tid>