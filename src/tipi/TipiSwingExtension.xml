<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension id="TipiSwing" requiresMain="" project="TipiSwing" version="0" title="Tipi Swing" vendor="Dexels" homepage="http://www.navajo.nl">
	<description>
		Standard swing implementation
	</description>
	<includes>
		<!--  needed for runtime, not for JNLP -->
		<include path="com/dexels/navajo/tipi/components/swingimpl/swingclassdef.xml"/>
	</includes>
	<jars>
		<jar path="TipiSwingClient.jar"/>
		<jar path="TimingFramework.jar"/>
		<jar path="AnimatedTransitions.jar"/>
		<jar path="TipiSwing.jar" main="tipi.MainApplication"/>
	</jars>
	<requires>
		<require path="Tipi" id="Tipi"/>
	</requires>
	<other>
		<link href="http://java.sun.com/docs/books/tutorial/ui/overview/intro.html"/>
		<link href="https://timingframework.dev.java.net/"/>
		<link href="https://animatedtransitions.dev.java.net/"/>

	</other>
	<main class="tipi.MainApplication" proxyjar="SwingMain.jar"/>
	
</extension>