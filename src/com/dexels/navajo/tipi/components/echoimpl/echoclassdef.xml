            <value direction="inout" name="use_checkbox" type="boolean"/>
            <value direction="in" name="visibleRowCount" type="integer"/>

            <value direction="inout" name="label_halign" type="selection">
                <option description="Leading" value="leading"/>
                <option description="Left" value="left"/>
                <option description="Center" value="center"/>
                <option description="Right" value="right"/>
                <option description="Trailing" value="trailing"/>
            </value>
            <value direction="inout" name="label_valign" type="selection" value="top">
                <option description="Top" value="top"/>
