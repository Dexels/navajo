                    <xsd:all>
                        <xsd:element name="FromCountryId" type="xsd:string"/>
                        <xsd:element name="ToCountryId" type="xsd:string"/>
                        <xsd:element name="Distance" type="xsd:integer"/>
                        <xsd:element name="DistanceKm" type="xsd:integer"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
        </xsd:schema>
    </types>

    <message name="ProcessReq">
        <part name="body" element="esns:ZipcodeDistance"/>
    </message>
    <message name="ProcessRes">
        <part name="body" element="esns:Result"/>
