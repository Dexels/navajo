    </binding>
    <service name="utils_ProcessGetZipcodeDistance">
        <documentation> Dit is de documentatie</documentation>
        <port binding="tns:utils_ProcessGetZipcodeDistance_Binding" name="utils_ProcessGetZipcodeDistance_Port">
