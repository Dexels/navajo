<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" activate="activate" immediate="true" name="tipi.dev.operation.build">
   <implementation class="com.dexels.navajo.tipi.dev.server.appmanager.operations.impl.JnlpBuild"/>
   <reference bind="addRepositoryInstance" cardinality="0..n" interface="com.dexels.navajo.repository.api.RepositoryInstance" name="RepositoryInstance" policy="dynamic" unbind="removeRepositoryInstance" target="(type=webstart)"/>
   <reference bind="setAppStoreManager" cardinality="1..1" interface="com.dexels.navajo.tipi.dev.server.appmanager.AppStoreManager" name="AppStoreManager" policy="dynamic" unbind="clearAppStoreManager"/>
   <reference bind="setRepositoryManager" cardinality="1..1" interface="com.dexels.navajo.repository.api.RepositoryManager" name="RepositoryManager" policy="dynamic" unbind="clearRepositoryManager"/>
   <service>
      <provide interface="com.dexels.navajo.repository.api.AppStoreOperation"/>
      <provide interface="javax.servlet.Servlet"/>
      <provide interface="org.osgi.service.event.EventHandler" />
   </service>
   <property name="osgi.command.scope" type="String" value="tipi"/>
   <property name="osgi.command.function" type="String">
		build
   </property>
   <property name="alias" type="String" value="/build"/>
   <property name="name" type="String" value="build"/>
   <property name="type" type="String" value="webstart"/>
   <property name="event.topics" type="String">
		repository/change
   </property>
</scr:component>
