<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="tipi.dev.githubservlet" immediate="true" enabled="true">
   <implementation class="com.dexels.navajo.tipi.dev.server.GitHubServlet"/>
   <reference bind="addApplicationStatus" cardinality="0..n" interface="com.dexels.navajo.tipi.dev.server.appmanager.ApplicationStatus" name="ApplicationStatus" policy="dynamic" unbind="removeApplicationStatus"/>
   <reference bind="addOperation" cardinality="0..n" interface="com.dexels.navajo.tipi.dev.server.appmanager.AppStoreOperation" name="AppStoreOperation" policy="dynamic" unbind="removeOperation"/>
   <reference bind="setApplicationManager" cardinality="1..1" interface="com.dexels.navajo.tipi.dev.server.appmanager.ApplicationManager" name="ApplicationManager" policy="dynamic" unbind="clearApplicationManager"/>
   
   <property name="alias" type="String" value="/github"/>
   <service>
      <provide interface="javax.servlet.Servlet"/>
   </service>
</scr:component>
