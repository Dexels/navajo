<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" activate="activate" name="tipi.dev.appstore.manager" immediate="true" configuration-policy="require">
   <implementation class="com.dexels.navajo.tipi.dev.server.appmanager.impl.AppStoreManagerImpl"/>
   <service>
      <provide interface="com.dexels.navajo.tipi.dev.server.appmanager.AppStoreManager"/>
   </service>
   <reference bind="addTipiSession" cardinality="0..n" interface="com.dexels.navajo.tipi.dev.server.websocket.TipiCallbackSession" name="TipiCallbackSession" policy="dynamic" unbind="removeTipiSession"/>
   <reference bind="addRepositoryInstance" cardinality="0..n" interface="com.dexels.navajo.repository.api.RepositoryInstance" name="RepositoryInstance" policy="dynamic" unbind="removeRepositoryInstance" target="(type=webstart)"/>
</scr:component>
