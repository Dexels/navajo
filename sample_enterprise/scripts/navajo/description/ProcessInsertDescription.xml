<?xml version="1.0" encoding="UTF-8"?>
<tsl xmlns="http://www.dexels.com/navascript" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dexels.com/navascript http://mail.dexels.com:8080/schema/navascript.xsd">
    <map object="com.dexels.navajo.adapter.SQLMap">
        <field name="datasource">
            <expression value="'navajostore'"/>
        </field>
        <field name="query">
            <expression xml:space="preserve">
                INSERT INTO propertydescription ( 
                descriptionid
                , name
                , locale
                , description
                , context
                , objectid
                , objecttype
                )
                VALUES (propertydescription_seq.nextval,?, ?, ?, ?, ?,?)
              
            </expression>
        </field>
        <field name="parameter">
            <expression value="[/Description/Name]"/>
        </field>
        <field name="parameter">
            <expression value="[/Description/Locale]"/>
        </field>
        <field name="parameter">
            <expression value="[/Description/Description]"/>
        </field>
        <field name="parameter">
            <expression value="[/Description/Context]"/>
        </field>
        <field name="parameter">
            <expression value="[/Description/ObjectId]"/>
        </field>
        <field name="parameter">
            <expression value="[/Description/ObjectType]"/>
        </field>
        <field name="doUpdate">
            <expression value="true"/>
        </field>
    </map>
</tsl>
