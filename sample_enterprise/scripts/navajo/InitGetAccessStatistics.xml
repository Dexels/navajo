<?xml version="1.0" encoding="UTF-8"?>
<tsl xmlns="http://www.dexels.com/navascript" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dexels.com/navascript http://mail.dexels.com:8080/schema/navascript.xsd">
    <methods>
        <method name="ProcessGetAccessStatistics">
            <required message="StatisticsParameters"/>
        </method>
    </methods>
    <message name="StatisticsParameters">
        <property name="View" type="selection" direction="in" cardinality="1">
            <option name="Year" value="Y" selected="0"/>
            <option name="Month" value="M" selected="0"/>
            <option name="Week" value="W" selected="0"/>
            <option name="Day" value="D" selected="0"/>
            <option name="Hour" value="H" selected="1"/>
        </property>
    </message>
</tsl>
