<?xml version="1.0" encoding="UTF-8"?>
<tsl xmlns="http://www.dexels.com/navascript" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dexels.com/navascript file:/home/arjen/projecten/Navajo/schemas/navascript.xsd">
  <map object="com.dexels.navajo.adapter.ScriptMap">
    <message name="NavajoScript">
      <property description="Author" direction="out" name="Author">
        <expression value="$author"/>
      </property>
      <property description="Id" direction="out" name="Id">
        <expression value="$id"/>
      </property>
      <property description="Notes" direction="out" name="Notes">
        <expression value="$notes"/>
      </property>
      <property description="Repository" direction="out" name="Repository">
        <expression value="$repository"/>
      </property>
      <message name="Methods" type="array">
        <map ref="methodlist">
          <property description="Method" name="Method" type="string" direction="out">
            <expression value="$name"/>
          </property>
        </map>
      </message>
    </message>
  </map>
</tsl>
