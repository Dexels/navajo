<?xml version="1.0" encoding="UTF-8"?>
<tsl>
    <message name="LockManager">
        <map object="com.dexels.navajo.lockguard.LockManager">
            <property name="Version" direction="out">
                <expression value="$VERSION"/>
            </property>
            <property name="LockCount" direction="out" description="Number of active locks">
                <expression value="$lockCount"/>
            </property>
            <message name="Definitions">
                <map ref="definitions">
                    <property name="LockDefinitionId" direction="out">
                        <expression value="$id"/>
                    </property>
                    <property name="MaxInstanceCount" direction="out">
                        <expression value="$maxInstanceCount"/>
                    </property>
                    <property name="WebservicePattern" direction="out">
                        <expression value="$webservice"/>
                    </property>
                    <property name="MatchUsername" direction="out">
                        <expression value="$matchUsername"/>
                    </property>
                    <property name="MatchRequest" direction="out">
                        <expression value="$matchRequest"/>
                    </property>
                </map>
            </message>
            <message name="Locks">
                <map ref="locks">
                    <property name="Id" direction="out">
                        <expression value="$lockId"/>
                    </property>
                    <property name="Definition" direction="out">
                        <expression value="$lockSignature"/>
                    </property>
                    <property name="Username" direction="out">
                        <expression value="$username"/>
                    </property>
                    <property name="InstanceCount" direction="out">
                        <expression value="$instanceCount"/>
                    </property>
                    <property name="Webservice" direction="out">
                        <expression value="$webservice"/>
                    </property>
                    <property name="Username" direction="out">
                        <expression value="$username"/>
                    </property>
                </map>
            </message>
        </map>
    </message>
</tsl>
