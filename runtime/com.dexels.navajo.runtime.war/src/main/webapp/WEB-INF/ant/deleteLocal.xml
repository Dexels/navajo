<!-- $Id: deleteLocal.xml,v 1.1 2009/09/08 16:23:44 frank Exp $ -->
<project default="createDefaultInstall" >
	<target name="createDefaultInstall">
		<echo message="Deleting local stuff: ${basedir} "/>
		<delete>
			<fileset dir=".">
				<include name="adapters/**"/>
				<include name="config/**"/>
				<include name="scripts/**"/>
			</fileset>
		</delete>
	</target>
</project>