<?xml version="1.0" encoding="UTF-8" ?>
<!-- $Id$ -->
<project name="NavajoJsp" default="archive" basedir=".">
  <property name="src" location="src"/>
  <property name="build" location="classes"/>
  <property name="lib" value="lib"/>
  <property name="archive" value="${ant.project.name}.jar"/>

  <target name="archive">
  	<echo message="Staring build"/>
    <jar jarfile="${archive}">
      <fileset dir="${build}"/>
    </jar>
  	<copy file="${archive}" todir="../GeoJsp/WebContent/WEB-INF/lib"/>
  </target>
</project>
