<project name="NavajoJsp" default="archive" basedir=".">
  <property name="src" location="src"/>
  <property name="build" location="classes"/>
  <property name="lib" value="lib"/>
  <property name="archive" value="WEB-INF/lib/${ant.project.name}.jar"/>

  <target name="archive">
    <jar jarfile="${archive}">
      <fileset dir="${build}"/>
    </jar>
  </target>
</project>
