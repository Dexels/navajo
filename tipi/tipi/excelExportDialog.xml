<tid xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="tipiscript.xsd" errorhandler="error">
    <tipi name="excelExportDialog" id="excelExportDialog" class="dialog" w="400" title="&apos;Exporteren&apos;" h="200" modal="true">
        <event type="onInstantiate">
            <action type="performTipiMethod">
                <param name="path" value="{tipi:/.}"/>
                <param name="name" value="&apos;show&apos;"/>
            </action>
        </event>
        <layout type="gridbag">
            <component-instance id="filechooser" class="filechooser" constraint="0,0,2,1,1.0,0.0,18,2,5,10,0,0,0,0"/>
            <component-instance id="okButton" class="button" icon="{resource:/ok.png}" constraint="0,1,1,1,0.0,0.0,18,0,5,10,0,0,0,0">
                <event type="onActionPerformed">
                    <block expression="{attribute:/../filechooser:file} != null AND {attribute:/../filechooser:file} != &apos;&apos;">
                        <action type="performTipiMethod">
                            <param name="delimiter" value="&apos;,&apos;"/>
                            <param name="path" value="{component:/init/tipi/tabs/dataTab/sqlPanel/tipitable}"/>
                            <param name="filename" value="{attribute:/../filechooser:file}"/>
                            <param name="name" value="&apos;export&apos;"/>
                        </action>
                        <action type="dispose">
                            <param name="path" value="{tipi:/..}"/>
                        </action>
                    </block>
                </event>
            </component-instance>
            <component-instance id="cancelButton" class="button" icon="{resource:/cancel.png}" constraint="1,1,1,1,0.0,0.0,17,0,5,10,0,0,0,0">
                <event type="onActionPerformed">
                    <action type="dispose">
                        <param name="path" value="{tipi:/..}"/>
                    </action>
                </event>
            </component-instance>
        </layout>
    </tipi>
</tid>
