<tid xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../tipi.xsd">
    <!-- constraints & id have to be added at instantiate time -->
    <d.panel name="tabPersonWindowPersonRemarks" applyCss="true">
        <l.mig grid="wrap 1, ins 2, fill">
            <c.panel id="textRemarks" cssClass="'borderTabRemarksTextRemarks'" constraint="grow" opaque="false">
                <l.mig grid="wrap 2, ins 5, fill">
                    <c.property id="Remarks1" label_indent="150" propertyname="'/MemberData/Remarks1'" constraint="width 45%" />
                    <c.property id="Remarks2" label_indent="150" propertyname="'/MemberData/Remarks2'" constraint="width 45%" />
                    <c.property id="Remarks3" label_indent="150" propertyname="'/MemberData/Remarks3'" constraint="width 45%" />
                    <c.property id="Remarks4" label_indent="150" propertyname="'/MemberData/Remarks4'" constraint="width 45%" />
                    <c.property id="Remarks5" label_indent="150" propertyname="'/MemberData/Remarks5'" constraint="width 45%" />
                    <c.property id="Remarks6" label_indent="150" propertyname="'/MemberData/Remarks6'" constraint="width 45%" />
                </l.mig>
            </c.panel>
            <c.panel id="dateRemarks" cssClass="'borderTabRemarksDateRemarks'" constraint="grow" opaque="false">
                <l.mig grid="wrap 2, ins 5, fill">
                    <c.property id="Remarks7" label_indent="150" propertyname="'/MemberData/Remarks7'" constraint="width 230" showdatepicker="true" />
                    <c.property id="Remarks8" label_indent="150" propertyname="'/MemberData/Remarks8'" constraint="width 230" showdatepicker="true" />
                </l.mig>
            </c.panel>
            <c.panel id="dropdownRemarks" cssClass="'borderTabRemarksDropdownRemarks'" constraint="grow" opaque="false">
                <l.mig grid="wrap 1, ins 5, fill">
                    <c.property id="Remarks9" label_indent="150" propertyname="'/MemberData/Remarks9'" constraint="width 100%" />
                    <c.property id="Remarks10" label_indent="150" propertyname="'/MemberData/Remarks10'" constraint="width 100%" />
                </l.mig>
            </c.panel>
            <c.panel id="personRemarksPanel" cssClass="'borderTabRemarksOtherRemarks'" constraint="grow" opaque="false">
                <l.mig grid="wrap 1, ins 5, fill">
                    <c.property id="PersonRemarksTextArea" cssClass="'propertyPersonRemarks'" showlabel="false" propertyname="'/MemberData/PersonRemarks'" visibleRowCount="4" memoRowCount="4" constraint="height 90%, width 100%"/>
                </l.mig>
            </c.panel>
        </l.mig>
    </d.panel>
</tid>
