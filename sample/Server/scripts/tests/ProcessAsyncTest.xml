<tsl debug="response">
  <map name="async" object="com.dexels.navajo.mapping.AsyncTest">
    <request>
      <field name="d">
        <expression value="[/Input/d]"/>
      </field>
      <field name="iter">
        <expression value="[/Input/iter]"/>
      </field>
      <message name="StartedThread"/>
