<tml
  id="euro"
  author="Arjen Schoneveld" 
  notes="Euro Calculator, supports German Marks,
    French Francs, Belgium Francs and Dutch Gulders
    to Euro calculations"
  repository="$Id$">
  <message name="apenoot">
    <property name="getal" type="float" length="5" description="Geef een getal" value="" direction="in"/>
  </message>
  <message name="invoer">
    <property name="bedrag" type="integer" description="Bedrag" length="10" value="" direction="in"/>
    <property name="iets" type="float" description="Iets" length="10" value="" direction="in"/>
    <property name="product" type="string" description="Product naam" length="10" value="" direction="in"/>
    <property cardinality="1" name="valuta" type="selection" length="" description="Gekozen valuta" value="" direction="in">
      <option name="euro" selected="1" value="0"/>
      <option name="gulden" selected="0" value="1"/>
      <option name="DM" selected="0" value="2"/>
      <option name="FF" selected="0" value="3"/>
      <option name="BEF" selected="0" value="4"/>
      <option name="USD" selected="0" value="5"/>
    </property>
  </message>
  <message name="aap">
    <property name="martin" type="string" description="Iets" length="10" value="" direction="in"/>
  </message>
  <methods>
    <method name="euro_verstuur">
      <required message="invoer"/>
    </method>
  </methods>
</tml>
